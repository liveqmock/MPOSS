<?xml version="1.0" encoding="ISO-8859-1"?>

<decorators defaultdir="/WEB-INF/jsp">
	<!-- Any urls that are excluded will never be decorated by Sitemesh -->
	<excludes>
		<pattern>/styles/*</pattern>
		<pattern>/content/upload/*</pattern>
		<pattern>/management*</pattern>
		<pattern>/account*</pattern>
		<pattern>/admin</pattern>
		<pattern>/login</pattern>
		<pattern>/doLogin</pattern>
		<pattern>/goods/product/goImport</pattern>
		<pattern>/goods/product/import</pattern>
		<pattern>/partner/consumer/goImport</pattern>
		<pattern>/partner/consumer/import</pattern>
		<pattern>/partner/provider/goImport</pattern>
		<pattern>/partner/provider/import</pattern>
		<pattern>/sale/make/goImport</pattern>
		<pattern>/sale/make/import</pattern>
	</excludes>
	
	<decorator name="www" page="layout/template.jsp">
		<pattern>/*</pattern>
	</decorator>
	
</decorators>