package dwz.business.system;

import java.io.Serializable;

import dwz.framework.sys.business.AbstractBusinessObject;
import dwz.persistence.beans.SysOrg;

public class Org extends AbstractBusinessObject {
	
	private SysOrg sysOrg;
	
	public Org(){
		this.sysOrg = new SysOrg();
	}
	
	public Org(SysOrg sysOrg) {
		this.sysOrg = sysOrg;
	}

	public SysOrg getSysOrg() {
		return sysOrg;
	}

	public void setSysOrg(SysOrg sysOrg) {
		this.sysOrg = sysOrg;
	}

	@Override
	public Serializable getId() {
		return this.sysOrg.getOrgId();
	}
	
	public Integer getOrgId() {
		return this.sysOrg.getOrgId();
	}
	public void setOrgId(Integer orgId) {
		this.sysOrg.setOrgId(orgId);
	}
	public String getOrgName() {
		return this.sysOrg.getOrgName();
	}
	public void setOrgName(String orgName) {
		this.sysOrg.setOrgName(orgName);
	}
	public String getEngName() {
		return this.sysOrg.getEngName();
	}
	public void setEngName(String engName) {
		this.sysOrg.setEngName(engName);
	}
	public String getTel() {
		return this.sysOrg.getTel();
	}
	public void setTel(String tel) {
		this.sysOrg.setTel(tel);
	}
	public String getFax() {
		return this.sysOrg.getFax();
	}
	public void setFax(String fax) {
		this.sysOrg.setFax(fax);
	}
	public String getAddress() {
		return this.sysOrg.getAddress();
	}
	public void setAddress(String address) {
		this.sysOrg.setAddress(address);
	}
	public String getEngAddress() {
		return this.sysOrg.getEngAddress();
	}
	public void setEngAddress(String engAddress) {
		this.sysOrg.setEngAddress(engAddress);
	}
	public String getCurrPlatServiceType() {
		return this.sysOrg.getCurrPlatServiceType();
	}
	public void setCurrPlatServiceType(String currPlatServiceType) {
		this.sysOrg.setCurrPlatServiceType(currPlatServiceType);
	}
	public String getServiceEndDate() {
		return this.sysOrg.getServiceEndDate();
	}
	public void setServiceEndDate(String serviceEndDate) {
		this.sysOrg.setServiceEndDate(serviceEndDate);
	}
	public String getInTakingFlag() {
		return this.sysOrg.getInTakingFlag();
	}
	public void setInTakingFlag(String inTakingFlag) {
		this.sysOrg.setInTakingFlag(inTakingFlag);
	}
	public String getLogo() {
		return this.sysOrg.getLogo();
	}
	public void setLogo(String logo) {
		this.sysOrg.setLogo(logo);
	}

}
